<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html  xmlns="http://www.w3.org/1999/xhtml"
	xmlns:th="http://www.thymeleaf.org"
	xmlns:sec="http://www.thymeleaf.org/extras/spring-security">

<head>
  	<link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link type="text/css" rel="stylesheet" href="/materialize/css/materialize.min.css"  media="screen,projection"/>
    <link type="text/css" rel="stylesheet" href="/materialize/css/personalizado.css"  media="screen,projection"/>

    
	<meta name="viewport" content="width=device-width, initial-scale=1.0"/>
<title>Cadastro Login</title>
</head>
<body>
	<div class="container">
		<h3>Login Cadastro Pessoas</h3>
		
		<form th:action="@{login}" method= "post" class= "col s12">
		
		<div class="row">
			<div class="input-field col s6">
				<label class="active">Login raiz</label>
				<input class="validate" type="text" name="username"></input>
			</div>
			<div class="col s6">

			</div>	
		</div>
		<div class="row">
			<div class="input-field col s6">
				<label class="active">Senha</label>
				<input class="validate" type="password" name="password"></input>
			</div>
			<div class="col s6">

			</div>			
		</div>
			<div class="input-field col s6">
				
				<input  type="submit" name="password" value="logar" class="waves-light btn green" ></input>				
			</div>
			
			
		</form>
		
		<br />
		<a class="waves-light btn blue"  href="/cadastrologin">Cadastrar Login</a>
		
		

		
	</div>
	<script type="text/javascript" src="/materialize/js/materialize.min.js"></script>
	    <!-- Compiled and minified JavaScript -->
   
</body>
</html>